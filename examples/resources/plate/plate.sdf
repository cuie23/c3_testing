<?xml version="1.0"?>
<sdf version="1.7">
  <world name="plate_world">

    <model name="plate_model">

      <!-- Plate -->
      <link name="plate">
        <pose>0 0 0 0 0 0</pose>
        <inertial><mass>0.5</mass></inertial>
        <visual name="plate_visual">
          <geometry>
            <cylinder>
              <radius>0.5</radius>
              <length> 0.02 </length>
            </cylinder>
          </geometry>
          <material><diffuse>0.1 0.1 0.1 1</diffuse></material>
        </visual>
        <collision name="plate_collision">
          <geometry>
            <cylinder>
              <radius>0.5</radius>
              <length> 0.02 </length>
            </cylinder>
          </geometry>
          <drake:proximity_properties>
          <drake:compliant_hydroelastic/>
            <drake:hydroelastic_modulus> 3.0e7 </drake:hydroelastic_modulus>
            <drake:mesh_resolution_hint> 0.05 </drake:mesh_resolution_hint>
            <drake:hunt_crossley_dissipation>10</drake:hunt_crossley_dissipation>
            <drake:mu_dynamic>0.4</drake:mu_dynamic>
          </drake:proximity_properties>
        </collision>
      </link>



      <link name="link_x"/>
      <joint name="plate_x" type="prismatic">
        <parent>world</parent>
        <child>link_x</child>
        <axis><xyz>1 0 0</xyz></axis>
      </joint>

      <link name="link_y"/>
      <joint name="plate_y" type="prismatic">
        <parent>link_x</parent>
        <child>link_y</child>
        <axis><xyz>0 1 0</xyz></axis>
      </joint>

      <link name="link_z"/>
      <joint name="plate_z" type="prismatic">
        <parent>link_y</parent>
        <child>link_z</child>
        <axis><xyz>0 0 1</xyz></axis>
      </joint>

      <link name="link_roll"/>
      <joint name="plate_roll" type="revolute">
        <parent>link_z</parent>
        <child>link_roll</child>
        <axis>
          <xyz>1 0 0</xyz>
          <limit><lower>-0.3</lower><upper>0.3</upper></limit>
        </axis>
      </joint>

      <joint name="plate_pitch" type="revolute">
        <parent>link_roll</parent>
        <child>plate</child>
        <axis>
          <xyz>0 1 0</xyz>
          <limit><lower>-0.3</lower><upper>0.3</upper></limit>
        </axis>
      </joint>

    </model>

  </world>
</sdf>
